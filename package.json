{
  "name": "ts-ip2region2-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for ts-ip2region2 packages",
  "private": true,
  "scripts": {
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "sync": "cd packages/ts-ip2region2-data && npm run sync",
    "sync-check": "cd packages/ts-ip2region2-data && npm run sync-check",
    "semantic-release:dry": "pnpm -r --workspace-concurrency=1 exec -- node ../../scripts/release.mjs --dry-run",
    "semantic-release": "pnpm -r --workspace-concurrency=1 exec -- node ../../scripts/release.mjs"
  },
  "keywords": [
    "typescript",
    "ip2region",
    "geolocation",
    "ipv4",
    "ipv6",
    "native-addon",
    "high-performance",
    "xdb",
    "ip-lookup"
  ],
  "author": "Steven Qiang <git@qiangmou.ren>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Steven-Qiang/ts-ip2region2.git"
  },
  "bugs": {
    "url": "https://github.com/Steven-Qiang/ts-ip2region2/issues"
  },
  "homepage": "https://github.com/Steven-Qiang/ts-ip2region2#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^24.8.1",
    "semantic-release": "^24.2.9",
    "semantic-release-monorepo": "^8.0.2",
    "semver": "^7.7.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
